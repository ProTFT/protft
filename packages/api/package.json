{
  "name": "protft_api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "yarn nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "yarn node dist/main --max-old-space-size=512",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.388.0",
    "@aws-sdk/lib-dynamodb": "^3.388.0",
    "@nestjs/apollo": "^10.0.11",
    "@nestjs/common": "patch:@nestjs/common@npm:8.4.6#../../.yarn/patches/@nestjs-common-npm-8.4.6-ceb145a01e.patch",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/graphql": "^10.0.17",
    "@nestjs/jwt": "^10.0.1",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/swagger": "^7.1.1",
    "@nestjs/typeorm": "^8.0.3",
    "apollo-server-express": "^3.8.1",
    "bcrypt": "^5.1.0",
    "cookie-parser": "^1.4.6",
    "country-code-lookup": "^0.0.22",
    "graphql": "^16.5.0",
    "joi": "^17.9.2",
    "lodash.chunk": "^4.2.0",
    "luxon": "^3.3.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "slugify": "^1.6.5",
    "ts-morph": "^14.0.0",
    "typeorm": "^0.2.45",
    "webpack": "^5.72.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.13",
    "@types/jest": "27.5.0",
    "@types/lodash.chunk": "^4.2.7",
    "@types/luxon": "^3.3.0",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.0.0",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.34",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "28.0.3",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.1",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.0.0",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.(spec|e2e-spec)\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "src/**/*.(t|j)s",
      "!**/*.module.ts",
      "!src/db/migrations/*",
      "!**/*.entity.ts",
      "!src/graphql.ts"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
